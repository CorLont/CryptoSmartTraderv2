modules = ["python-3.11"]

[nix]
channel = "stable-25_05"
packages = ["gcc", "glibcLocales", "libcxx", "libjpeg_turbo", "libpng", "libxcrypt", "ocl-icd", "opencl-headers", "pkg-config", "which", "xsimd", "zip"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "TestApp"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "WorkingApp"

[[workflows.workflow]]
name = "TestApp"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run test_app_simple.py --server.port 5001"
waitForPort = 5001

[[workflows.workflow]]
name = "WorkingApp"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "streamlit run app_working.py --server.port 5000"
waitForPort = 5000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3003

[[ports]]
localPort = 5002
externalPort = 6000

[[ports]]
localPort = 8000
externalPort = 8000

[[ports]]
localPort = 8001
externalPort = 3000

[[ports]]
localPort = 8002
externalPort = 3002

[[ports]]
localPort = 8003
externalPort = 3001

[[ports]]
localPort = 8090
externalPort = 4200

[[ports]]
localPort = 8091
externalPort = 5000

[[ports]]
localPort = 8092
externalPort = 5173

[agent]
integrations = ["web_scraper==1.0.0"]
