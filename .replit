modules = ["python-3.11"]

[nix]
channel = "stable-25_05"
packages = ["gcc", "glibcLocales", "libcxx", "libjpeg_turbo", "libpng", "libxcrypt", "libyaml", "ocl-icd", "opencl-headers", "pkg-config", "which", "xsimd", "zip"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "SecretsManagement"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "AIGovernanceDashboard"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "SimpleAIDashboard"

[[workflows.workflow.tasks]]
