args = "MLDisciplineDashboard"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "EnhancedAlphaMotorDashboard"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "WhaleSentimentDashboard"

[[workflows.workflow]]
name = "SecretsManagement"
author = "agent"

[workflows.workflow.metadata]
